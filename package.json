{
  "name": "angularjs-webapp-bootstrap-starter",
  "version": "1.0.0",
  "description": "An WebApp Bootstrap Starter",
  "keywords": [],
  "author": "Shawn Dao <daominhsangvn@gmail.com>",
  "homepage": "https://github.com/daominhsangvn/angularjs-es6-webpack2-seed",
  "license": "MIT",
  "scripts": {
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile",
    "build:docker": "npm run build:prod && docker build -t angular2-webpack-start:latest .",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build": "npm run build:dev",
    "ci": "npm run lint && npm test && npm run e2e",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean:start": "npm start",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "docker": "docker",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "npm run protractor",
    "github-deploy:dev": "webpack --config config/webpack.github-deploy.js --progress --profile --github-dev",
    "github-deploy:prod": "webpack --config config/webpack.github-deploy.js --progress --profile --github-prod",
    "github-deploy": "npm run github-deploy:dev",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "postversion": "git push && git push --tags",
    "prebuild:dev": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist",
    "preclean:install": "npm run clean",
    "preclean:start": "npm run clean",
    "pree2e": "npm run webdriver:update -- --standalone",
    "preversion": "npm test",
    "protractor": "protractor",
    "rimraf": "rimraf",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist --cors",
    "server": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "start": "npm run server:dev",
    "test": "karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "npm run build",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch": "npm run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "build:production": "npm run clean:dist && webpack --config config/webpack.prod.js  --progress --profile --bail --env.profile=production",
    "build:staging": "npm run clean:dist && webpack --config config/webpack.prod.js  --progress --profile --bail --env.profile=staging",
    "deploy:staging": "node deploy staging",
    "deploy:production": "node deploy production",
    "build:deploy:staging": "npm run build:staging && node deploy staging",
    "build:deploy:production": "npm run build:production && node deploy production"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-animate": "^1.5.8",
    "angular-breadcrumb": "^0.4.1",
    "angular-cookies": "^1.5.8",
    "angular-loading-bar": "^0.9.0",
    "angular-local-storage": "^0.5.0",
    "angular-messages": "^1.5.8",
    "angular-mock": "^1.0.0",
    "angular-promise-buttons": "^0.1.13",
    "angular-sanitize": "^1.5.8",
    "angular-translate": "^2.12.0",
    "angular-translate-loader-partial": "^2.12.0",
    "angular-ui-bootstrap": "^2.1.3",
    "angular-ui-router": "^0.3.1",
    "angularjs-toaster": "^2.0.0",
    "animate.css": "^3.5.2",
    "autosize": "^3.0.17",
    "bootstrap": "^4.0.0-alpha.3",
    "flot": "^0.8.0-alpha",
    "font-awesome": "^4.6.3",
    "icheck": "^1.0.2",
    "jquery": "^3.1.0",
    "jqvmap": "^1.5.1",
    "lodash": "^4.15.0",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "material-design-icons-iconfont": "^3.0.0",
    "moment": "^2.14.1",
    "ng-file-upload": "^12.2.5",
    "oclazyload": "^1.0.9"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.8",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.24.0",
    "eslint": "^3.4.0",
    "eslint-config-angular": "^0.5.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-angular": "^1.3.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "ftp": "^0.3.10",
    "ftp-client": "^0.2.2",
    "ftp-deploy": "^1.1.0",
    "html-webpack-plugin": "^2.21.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0 ",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "ng-annotate-loader": "0.0.10",
    "node-sass": "^3.9.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.11.1",
    "promise": "^7.1.1",
    "protractor": "^3.2.2",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.1",
    "source-map-loader": "^0.1.5",
    "string-replace-loader": "^1.0.3",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.21",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^2.1.0-beta.2",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daominhsangvn/angularjs-es6-webpack2-seed.git"
  },
  "bugs": {
